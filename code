library(dplyr)
library(tidyr)
filter(titanic_original, titanic_original$embarked == "")
titanic1 <- titanic_original %>% mutate(embarked = gsub("^$", "S", embarked))
filter(titanic1, titanic1$embarked == "")
mean(titanic1$age, na.rm = TRUE)
titanic1$age[is.na(titanic1$age)] <- mean(titanic1$age, na.rm = TRUE)
titanic1 <- titanic1 %>% mutate(boat = gsub("^$", "NA", boat))
titanic1 <- titanic1 %>% mutate(has_cabin_number = ifelse(grepl("^$",cabin),0,1))
write.csv(titanic1, "titanic_clean.csv")
